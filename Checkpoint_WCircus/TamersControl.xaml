<UserControl x:Class="Checkpoint_WCircus.TamersControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Checkpoint_WCircus"
             mc:Ignorable="d"
             d:DesignHeight="400" d:DesignWidth="800">
    <Grid Height="400" Width="800">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*" />
                <RowDefinition Height="4*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="4*"/>
                <ColumnDefinition Width="4*"/>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>

            <!--<StackPanel Grid.Row="0" Orientation="Horizontal" HorizontalAlignment="Center">
                <Button x:Name="previousImageButton" Padding="15" Margin="15">Previous</Button>
                <Button x:Name="nextImageButton" Click="nextImageButton_Click" Padding="15" Margin="15">Next</Button>
            </StackPanel>-->
        </Grid>
    </Grid>
</UserControl>
